{
  "name": "aws-multi-account-bootstrap-monorepo",
  "version": "2.0.0-alpha.1",
  "private": true,
  "description": "AWS multi-account setup with Go backend + multi-platform frontends",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/aws-multi-account-bootstrap.git"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "go:build": "cd go && make build",
    "go:test": "cd go && make test",
    "bash:test": "cd bash && ./tests/test-config-simple.sh && ./tests/test-mock-adapters.sh"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "prettier": "^3.1.1",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
